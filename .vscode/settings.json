{
    "cSpell.words": [
      "antfu",
      "demi",
      "formkit",
      "gmap",
      "iconify",
      "intlify",
      "nuxt",
      "nuxtjs",
      "pinia",
      "pnpm",
      "Svgo",
      "unocss",
      "unplugin",
      "Vite",
      "vitejs",
      "Vitesse",
      "vitest",
      "vueuse",
      "composable",
      "composables",
      "defu",
      "postcss",
      "nuxt"
    ],
    "i18n-ally.sourceLanguage": "pl",
    "i18n-ally.displayLanguage": "pl",
    "i18n-ally.keystyle": "nested",
    "i18n-ally.localesPaths": "locale",
    "i18n-ally.sortKeys": true,
    "i18n-ally.enabledParsers": [
      "yaml"
      ],
    "i18n-ally.editor.preferEditor": true,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "files.associations": {
      "*.css": "tailwindcss"
    },
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[vue]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "eslint.format.enable": true,
    "cSpell.ignorePaths": ["locale", "node_modules", ".git", ".vscode"],
    "editor.quickSuggestions": {
      "strings": true
    },
    "eslint.validate": ["javascript", "javascriptreact", "yaml", "yml"]
  }
  